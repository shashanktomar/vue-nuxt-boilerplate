[ignore]
./nuxt/*
<PROJECT_ROOT>/node_modules/.*config-chain/test/broken.json

[include]
<PROJECT_ROOT>/client
<PROJECT_ROOT>/

[libs]
./node_modules/@vue-flow-typed/vuex/definitions
./node_modules/@vue-flow-typed/vue-router/definitions

[lints]
all=warn
unclear-type=error
untyped-import=off
untyped-type-import=error
unsafe-getters-setters=off
sketchy-null=error

[options]
include_warnings=true
esproposal.decorators=ignore
module.name_mapper='^~' -> '<PROJECT_ROOT>/client'
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=client
module.file_ext=.vue
module.file_ext=.js
